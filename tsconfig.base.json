{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@lib/antdapp": ["libs/antdapp/src/index.ts"],
      "@lib/muiapp": ["libs/muilib/src/index.ts"],
      "@lib/prime": ["libs/prime-lib/src/index.ts"],
      "@lib/redux": ["libs/redux/src/index.ts"],
      "@lib/shared": ["libs/shared/src/index.ts"],
      "@lib/shared/hoks": ["libs/shared/hoks/src/index.ts"],
      "@lib/shared/hooks": ["libs/shared/hooks/src/index.ts"],
      "@lib/shared/permissions": ["libs/shared/permissions/src/index.ts"],
      "@lib/shared/types": ["libs/shared/types/src/index.ts"],
      "@lib/shared/ui": ["libs/shared/ui/src/index.ts"],
      "@lib/shared/utils": ["libs/shared/utils/src/index.ts"],
      "@lib/tailwind": ["libs/tailwindapp/src/index.ts"],
      "@libs/semantic": ["libs/semantic-lib/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
